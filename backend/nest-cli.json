{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/streamline/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/streamline/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/streamline",
  "projects": {
    "streamline": {
      "type": "application",
      "root": "apps/streamline",
      "entryFile": "main",
      "sourceRoot": "apps/streamline/src",
      "compilerOptions": {
        "tsConfigPath": "apps/streamline/tsconfig.app.json"
      }
    },
    "auth-plugin": {
      "type": "application",
      "root": "apps/auth-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/auth-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-plugin/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "user-plugin": {
      "type": "application",
      "root": "apps/user-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/user-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-plugin/tsconfig.app.json"
      }
    },
    "procurement-plugin": {
      "type": "application",
      "root": "apps/procurement-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/procurement-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/procurement-plugin/tsconfig.app.json"
      }
    },
    "crm-plugin": {
      "type": "application",
      "root": "apps/crm-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/crm-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crm-plugin/tsconfig.app.json"
      }
    },
    "accounts-branch-plugin": {
      "type": "application",
      "root": "apps/accounts-branch-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/accounts-branch-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/accounts-branch-plugin/tsconfig.app.json"
      }
    },
    "transport-plugin": {
      "type": "application",
      "root": "apps/transport-plugin",
      "entryFile": "main",
      "sourceRoot": "apps/transport-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transport-plugin/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    }
  }
}